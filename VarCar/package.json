{
  "name": "figmap",
  "version": "0.1.0",
  "description": "FigMap - Figma Variables Orchestration & Intelligent Color Scale Generation",
  "private": true,
  "scripts": {
    "build": "npm run build:ui && npm run build:code && node build-plugin.js",
    "build:code": "esbuild src/code.ts --bundle --outfile=dist/code.js --format=iife --platform=browser --target=es6",
    "build:ui": "vite build",
    "dev": "npm run build",
    "watch": "npm run build",
    "size": "size-limit",
    "size:why": "size-limit --why"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@fontsource/geist-mono": "^5.2.7",
    "@fontsource/geist-sans": "^5.2.5",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@types/dagre": "^0.7.53",
    "@xyflow/react": "^12.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "colord": "^2.9.3",
    "dagre": "^0.8.5",
    "lucide-react": "^0.562.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@figma/plugin-typings": "^1.90.0",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "esbuild": "^0.19.0",
    "postcss": "^8.4.32",
    "size-limit": "^11.2.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-singlefile": "^0.13.5"
  },
  "size-limit": [
    {
      "path": "dist/ui/index.html",
      "limit": "800 KB"
    },
    {
      "path": "dist/code.js",
      "limit": "200 KB"
    }
  ]
}
